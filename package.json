{
  "name": "debugjudgeapp",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "eclint": "eclint check .",
    "tslint": "npm run-script tslint-app && npm run-script tslint-e2e",
    "tslint-app": "tslint \"src/main/**/*.ts\" --project src/main/angular/tsconfig.json --type-check",
    "tslint-e2e": "tslint \"src/test/angular/e2e/**/*.ts\" --project src/test/angular/e2e/tsconfig.json --type-check",
    "test": "karma start ./karma.conf.js",
    "buildall": "npm run build-team; npm run build-judge",
    "build-team": "echo Building app \\\"teamapp\\\"; ng build --app teamapp",
    "build-judge": "echo Building app \\\"judgeapp\\\"; ng build --app judgeapp",
    "protoc": "npm run protoc-js && npm run protoc-ts",
    "protoc-js": "mkdir -p target/generated-sources/ts/proto && pbjs -t static-module -o target/generated-sources/ts/proto/competition_pb.js src/main/proto/competition.proto",
    "protoc-ts": "pbts -o target/generated-sources/ts/proto/competition_pb.d.ts target/generated-sources/ts/proto/competition_pb.js --no-comments",
    "watchbuild": "ng build --watch",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "e2e": "protractor ./protractor.conf.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/cdk": "^2.0.0-beta.12",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/material": "^2.0.0-beta.12",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "ace-builds": "^1.2.8",
    "angular2-autosize": "^1.0.1",
    "core-js": "^2.4.1",
    "eclint": "^2.4.2",
    "google-protobuf": "^3.3.0",
    "ng2-ace-editor": "^0.3.1",
    "protobufjs": "^6.8.0",
    "rxjs": "^5.5.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.9",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.2.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.3.4",
    "webpack-dev-server": "~2.7.1",
    "webpack": "~3.7.1",
    "autoprefixer": "^6.5.3",
    "css-loader": "^0.28.1",
    "cssnano": "^3.10.0",
    "exports-loader": "^0.6.3",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.29.0",
    "less-loader": "^4.0.5",
    "postcss-loader": "^1.3.3",
    "postcss-url": "^5.1.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.3",
    "source-map-loader": "^0.2.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "style-loader": "^0.13.1",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.6.2",
    "circular-dependency-plugin": "^3.0.0",
    "webpack-concat-plugin": "1.4.0",
    "copy-webpack-plugin": "^4.1.1"
  }
}
