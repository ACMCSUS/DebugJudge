{
  "name": "debugjudgeapp",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "eclint": "eclint check .",
    "tslint": "npm run-script tslint-app && npm run-script tslint-e2e",
    "tslint-app": "tslint \"src/main/**/*.ts\" --project src/main/angular/tsconfig.json --type-check",
    "tslint-e2e": "tslint \"src/test/angular/e2e/**/*.ts\" --project src/test/angular/e2e/tsconfig.json --type-check",
    "test": "karma start ./karma.conf.js",
    "buildall": "npm run build-team; npm run build-judge",
    "build-team": "echo Building app \\\"teamapp\\\"; ng build --app teamapp",
    "build-judge": "echo Building app \\\"judgeapp\\\"; ng build --app judgeapp",
    "protoc": "npm run protoc-js && npm run protoc-ts",
    "protoc-js": "mkdir -p src/main/angular/proto && pbjs -t static-module -o src/main/angular/proto/debugjudge_pb.js src/main/proto/*.proto",
    "protoc-ts": "pbts -o src/main/angular/proto/debugjudge_pb.d.ts src/main/angular/proto/debugjudge_pb.js --no-comments",
    "watchbuild": "ng build --watch",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "e2e": "protractor ./protractor.conf.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.7",
    "@angular/cdk": "^5.2.3",
    "@angular/common": "^5.2.7",
    "@angular/compiler": "^5.2.7",
    "@angular/core": "^5.2.7",
    "@angular/forms": "^5.2.7",
    "@angular/http": "^5.2.7",
    "@angular/material": "^5.2.3",
    "@angular/platform-browser": "^5.2.7",
    "@angular/platform-browser-dynamic": "^5.2.7",
    "@angular/router": "^5.2.7",
    "ace-builds": "^1.2.8",
    "core-js": "^2.4.1",
    "eclint": "^2.4.2",
    "google-protobuf": "^3.3.0",
    "ng-autosize": "^1.1.0",
    "ng2-ace-editor": "^0.3.4",
    "protobufjs": "^6.8.0",
    "rxjs": "^5.5.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.9",
    "@angular/compiler-cli": "^5.2.7",
    "@angular/language-service": "^5.2.7",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~9.4.6",
    "autoprefixer": "^8.1.0",
    "circular-dependency-plugin": "^4.4.0",
    "codelyzer": "~4.2.1",
    "copy-webpack-plugin": "^4.1.1",
    "css-loader": "^0.28.1",
    "cssnano": "^3.10.0",
    "exports-loader": "^0.7.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^3.0.4",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "less-loader": "^4.0.5",
    "postcss-loader": "^2.1.1",
    "postcss-url": "^7.3.1",
    "protractor": "~5.3.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.3",
    "source-map-loader": "^0.2.0",
    "style-loader": "^0.20.2",
    "stylus-loader": "^3.0.1",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "^2.4.2",
    "url-loader": "^1.0.1",
    "webpack": "^3.10.0",
    "webpack-concat-plugin": "2.4.2",
    "webpack-dev-server": "^2.7.1"
  }
}
