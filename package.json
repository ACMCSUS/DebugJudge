{
  "name": "debugjudge",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "protoc": "mkdirp src/main/angular/app/proto && npm run protocjs && npm run protocts",
    "protocjs": "pbjs -t static-module -o src/main/angular/app/proto/dbgjdg_pb.js src/main/proto/*.proto",
    "protocts": "pbts -o src/main/angular/app/proto/dbgjdg_pb.d.ts src/main/angular/app/proto/dbgjdg_pb.js",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.2.9",
    "@angular/cdk": "5.2.4",
    "@angular/common": "5.2.9",
    "@angular/compiler": "5.2.9",
    "@angular/core": "5.2.9",
    "@angular/forms": "5.2.9",
    "@angular/http": "5.2.9",
    "@angular/material": "5.2.4",
    "@angular/platform-browser": "5.2.9",
    "@angular/platform-browser-dynamic": "5.2.9",
    "@angular/router": "5.2.9",
    "@angular/service-worker": "5.2.9",
    "core-js": "2.5.3",
    "google-protobuf": "3.5.0",
    "long": "4.0.0",
    "marked": "^0.3.19",
    "ng2-ace-editor": "0.3.4",
    "protobufjs": "6.8.6",
    "rxjs": "5.5.7",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.7.3",
    "@angular/compiler-cli": "5.2.9",
    "@angular/language-service": "5.2.9",
    "mkdirp": "^0.5.1",
    "typescript": "2.5.3"
  }
}
