{
  "project": {
    "version": "1.0.0-beta.25.5",
    "name": "debugjudgeapp"
  },
  "apps": [
    {
      "name": "teamapp",
      "root": "src/main/angular",
      "outDir": "target/classes/app/team",
      "baseHref": "/team/",
      "assets": [
        "src/main/angular/assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "teamapp/main.ts",
      "test": "teamapp/test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "dbgjdg",
      "mobile": false,
      "styles": [
        "styles.css",
        "theme.scss"
      ],
      "scripts": [
        "../../../node_modules/ace-builds/src-min/ace.js",
        "../../../node_modules/ace-builds/src-min/mode-plain_text.js",
        "../../../node_modules/ace-builds/src-min/theme-chrome.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    },
    {
      "name": "judgeapp",
      "root": "src/main/angular",
      "outDir": "target/classes/judgeapp",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "judgeapp/main.ts",
      "test": "judgeapp/test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "dbgjdg",
      "mobile": false,
      "styles": [
        "styles.css",
        "theme.scss"
      ],
      "scripts": [
        "node_modules/ace-builds/src-min/ace.js",
        "node_modules/ace-builds/src-min/mode-plain_text.js",
        "node_modules/ace-builds/src-min/theme-chrome.js"
      ],
      "environmentSource": "src/main/angular/environments/environment.ts",
      "environments": {
        "dev": "src/main/angular/environments/environment.ts",
        "prod": "src/main/angular/environments/environment.prod.ts"
      }
    }
  ],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
